package back1032;
import java.util.*;

public class Main {

	public static void main(String args[]){
		
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		in.nextLine();
		int count = 0;
		char[][] arr = null;
		while ( count < N ){
			System.out.println("ют╥б: " );
			String s = in.nextLine();
			arr[count] = s.toCharArray(); 
			count++;
		}
	
		System.out.println(arr[0].length);
	}
}
