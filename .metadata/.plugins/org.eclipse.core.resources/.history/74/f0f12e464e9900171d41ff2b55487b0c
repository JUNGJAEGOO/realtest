package back1038;
import java.util.*;
public class Main {

	public static void main(String args[]){
		
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		int result;
		long start = 0;
		if ( N <= 10){
			start = N;
		}
		else{
			result = 10;
			start = 11;
			while (true){
				//System.out.println(start+"가 들어갑니다.");
				int plus = calc(start);
				//System.out.println(plus+"더하기");
				result += plus;
				start++;
				if ( result == N){ break; }
			}
	//999999
	//9701538
			
		}
		if(start > 10 )
		System.out.println(start-1);
		else
		System.out.println(start);
	}
	
	public static int calc(long x){
		boolean isgood = true;
		int count = 1;
		while( true ){   // 자릿수 찾기
			
			if ( x / Math.pow(10,count) == 0){
				 count--; break;
			}
			
			count ++ ;
			
		}
		long mok = (long) Math.pow(10,count);
		long before = x / mok;
		mok /= 10;
		while( true ){
			
		    long tmp = x / mok ;
		    if ( before <= tmp ){
		    	isgood = false; break;
		    }
		    x %= mok;
			mok /= 10;
		    
			
		}
		
		
		
		
		
		if ( isgood == true ){
		return 1;
		}
		else{
	    return 0;
		}
		
	}
}
