package back1620;
import java.util.*;
import java.io.*;
public class Main {
	public static void main (String args[]) throws IOException{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String MandN = br.readLine();
		StringTokenizer st;
		st = new StringTokenizer(MandN," ");
		int M = Integer.parseInt(st.nextToken());
		int N = Integer.parseInt(st.nextToken());
		String list[] = new String[100001];
		
		for ( int i = 0 ; i < M ; i++){
			list[i] = br.readLine();
		}
		
		while ( N>0){
			String tmp = br.readLine();
			int count = 0;
			
			if ( isnum(tmp)){
				int StoTMP = Integer.parseInt(tmp);
				System.out.println(list[StoTMP]);
			}else{
			while ( true ){
				count++;
				if ( list[count].equals(tmp)){
					System.out.println(count);
					break;
				}
			}
			}
			N--;
		}
	}
	
	public static boolean isnum(String tmp){
		try {
			Integer.parseInt(tmp);
			return true;
		}catch(Exception e){
			return false;
		}
		
	}
}
