package back2294;

public class Main {
	public static void main(String args[]){
		Scanner in = new Scanner ( System.in);
		int n = in.nextInt();
		int k = in.nextInt();
		int coin[] = new int[n];
		int dp[] = new int[k+1];
		for ( int i = 0 ; i < k ; i++){
			coin[i] = in.nextInt();
		}
		
		dp[0] = 1;
		for ( int i = 0 ; i < coin.length ; i++){
			dp[coin[i]] = 1;
		}
		
		for ( int i = 0 ; i < coin.length ; i++){
			int min = 1234567890;
			for ( int j = 1 ; j <= k ; j++){
				if ( j-coin[i]>=0){
					dp[j] = Math.min(dp[j],dp[j-coin[i]]+1);
				}
			}
		}
		
	}
}
