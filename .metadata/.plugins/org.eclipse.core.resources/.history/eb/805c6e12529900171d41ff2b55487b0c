package back1038;
import java.util.*;
public class Main {

	public static void main(String args[]){
		
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		int result;
		long start = 0;
		if ( N <= 10){
			start = N;
		}
		else{
			result = 10;
			start = 11;
			while (true){
				//System.out.println(start+"가 들어갑니다.");
				int plus = calc(start);
				//System.out.println(plus+"더하기");
				result += plus;
				start++;
				if ( result == N){ break; }
			}
	//999999
	//9701538
			
		}
		if(start > 10 )
		System.out.println(start-1);
		else
		System.out.println(start);
	}
	
	public static int calc(long x){
		boolean isgood = true;
		int count = 1;
		//System.out.println((long)(x / Math.pow(10,count)));
		
		while( true ){   // 자릿수 찾기
			
			if ( (int)(x / Math.pow(10,count)) == 0){
				 count--; 
				 break;
			}
			
			count ++ ;
			
		}
		System.out.println("자릿수는 "+count);
		long mok = (long) Math.pow(10,count);
		long before = (long) (x / mok);
		
		while( true ){
			if (x>= 10){
			x = x - before * mok ;
			if ( x==0 ){ break; }
			}
			 mok /= 10;
	         long tmp = (long)(x / mok) ;
		    if ( before <= tmp ){
		    	isgood = false; break;		    
		    }
		    before = tmp;
		   
		    if(mok == 1){ break;}
			
		    
			
		}
		
		//800 98720
		
		if ( isgood == true ){
		return 1;
		}
		else{
	    return 0;
		}
		
	}
}
